<h2>Register New Client</h2>
<div class="row">
  <div class="col-md-9">
    <form action="/sources" method="post">
      <div class="form-group">
        <label for="identifier">Client Name</label>
        <input type="identifier" class="form-control" id="identifier" name="identifier" placeholder="Name">
      </div>
      <div class="form-group">
        <label for="rootUrl">Root URL</label>
        <input type="rootUrl" class="form-control" id="rootUrl" name="rootUrl" placeholder="URL">
      </div>
      <button type="submit" class="btn btn-default">Submit</button>
    </form>
  </div>
</div>
